{
    "name": "com.xbrowsersync",
    "version": "1.2.2",
    "displayName": "xBrowserSync",
    "cordova": {
        "platforms": [
            "android",
            "ios"
        ],
        "plugins": {
            "com.greybax.spinnerdialog": {},
            "com.jcesarmobile.filepicker": {},
            "com.virtualartifacts.webintent": {},
